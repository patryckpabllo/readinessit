    <pd:activity name="PopulateErorResponse">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>587</pd:x>
        <pd:y>396</pd:y>
        <config>
            <processName>/CommonProcesses/ErrorHandlingProcess/ErrorResponseMapFromEOM.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <SourceSystem>
                    <xsl:value-of select="if(exists($ESI_GetEligibleService/ns8:root/ns8:ResponseCode)) then 'CRM' else 'TIBCO'"/>
                </SourceSystem>
                <ChannelName>
                    <xsl:value-of select="'Selfcare'"/>
                </ChannelName>
                <ChannelType>
                    <xsl:value-of select="'External'"/>
                </ChannelType>
                <xsl:choose xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
                    <xsl:when test="exists($_error_ESI_GetEligibleService/ns4:ExceptionRequest)">
                        <ResponseCode>
                            <xsl:value-of select="$_error_ESI_GetEligibleService/ns4:ExceptionRequest/ExceptionCode"/>
                        </ResponseCode>
                    </xsl:when>
                    <xsl:when test="exists($ESI_GetEligibleService/ns8:root/ns8:ResponseCode)">
                        <ResponseCode>
                            <xsl:value-of select="$ESI_GetEligibleService/ns8:root/ns8:ResponseCode"/>
                        </ResponseCode>
                    </xsl:when>
                    <xsl:when test="exists($_error/ns5:ErrorReport/MsgCode)">
                        <ResponseCode>
                            <xsl:value-of select="$_error/ns5:ErrorReport/MsgCode"/>
                        </ResponseCode>
                    </xsl:when>
                    <xsl:otherwise>
                        <ResponseCode>
                            <xsl:value-of select="9999"/>
                        </ResponseCode>
                    </xsl:otherwise>
                </xsl:choose>
                <xsl:choose xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
                    <xsl:when test="exists($_error_ESI_GetEligibleService/ns4:ExceptionRequest)">
                        <ResponseDescription>
                            <xsl:value-of select="$_error_ESI_GetEligibleService/ns4:ExceptionRequest/StackTrace"/>
                        </ResponseDescription>
                    </xsl:when>
                    <xsl:when test="exists($ESI_GetEligibleService/ns8:root/ns8:ResponseCode)">
                        <ResponseDescription>
                            <xsl:value-of select="$ESI_GetEligibleService/ns8:root/ns8:ResponseDescription"/>
                        </ResponseDescription>
                    </xsl:when>
                    <xsl:when test="exists($_error/ns5:ErrorReport/MsgCode)">
                        <ResponseDescription>
                            <xsl:value-of select="$_error/ns5:ErrorReport/Msg"/>
                        </ResponseDescription>
                    </xsl:when>
                    <xsl:otherwise>
                        <ResponseDescription>
                            <xsl:value-of select="'Uncategorized Error occured'"/>
                        </ResponseDescription>
                    </xsl:otherwise>
                </xsl:choose>
                <TIBCOServiceName>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/BusinessProcesses/Services/Common/ServiceGifting/GetEligibleServices/ServiceName"/>
                </TIBCOServiceName>
                <ErrorCategory>
                    <xsl:value-of select="'2'"/>
                </ErrorCategory>
                <TargetSystem>
                    <xsl:value-of select="''"/>
                </TargetSystem>
            </root>
        </pd:inputBindings>
    </pd:activity>
