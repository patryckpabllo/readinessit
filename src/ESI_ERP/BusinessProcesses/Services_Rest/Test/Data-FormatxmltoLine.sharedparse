<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://www.tibco.com/schemas/ESI_ERP/BusinessProcesses/Services/Test/Schema.xsd">
    <import namespace="http://www.tibco.com/schemas/ESI_ERP/BusinessProcesses/Services/Test/Schema.xsd" schemaLocation="/BusinessProcesses/Services/Test/Schema.xsd"/>
    <name>Data-FormatxmltoLine</name>
    <resourceType>ae.shared.ParseSharedResource</resourceType>
    <config xmlns:xsd="http://www.w3.org/2001/XMLSchema">
        <FormatType>Delimiter separated</FormatType>
        <ColumnSeparator>|</ColumnSeparator>
        <ColSeparatorRule>Treat all characters as entered as a single column separator string</ColSeparatorRule>
        <LineSeparator>;</LineSeparator>
        <FillCharacter>Space</FillCharacter>
        <LineLength>3</LineLength>
        <DataFormat>
            <xsd:element name="root">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="param" type="xsd:string"/>
                        <xsd:element name="param1" type="xsd:string"/>
                        <xsd:element name="param2" type="xsd:string"/>
                        <xsd:element name="param3" type="xsd:string"/>
                        <xsd:element name="param4" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </DataFormat>
    </config>
</BWSharedResource>