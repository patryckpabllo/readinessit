				<xs:element name="USERERRORCODE" type="xs:string" minOccurs="0"/>
				<xs:element name="USERERRORDESC" type="xs:string" minOccurs="0"/>
				<xs:element name="CHANNELERRORCODE" type="xs:string" minOccurs="0"/>
				<xs:element name="CHANNELERRORDESC" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>