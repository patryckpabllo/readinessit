<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.ericsson.com/ZainKW/BusinessSchemas/Services/Applications/ConceptWave/CreateOrder/CreateOrderRequest.xsd"
	 targetNamespace="http://www.ericsson.com/ZainKW/BusinessSchemas/Services/Applications/ConceptWave/CreateOrder/CreateOrderRequest.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="CreateOrderRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="orderHeader" minOccurs="0"/>
				<xs:element ref="customerRead" minOccurs="0"/>
				<xs:element ref="contractNew" minOccurs="0"/>
				<xs:element ref="contractDeviceAdd" minOccurs="0"/>
				<xs:element ref="contractServicesAdd" minOccurs="0"/>
				<xs:element ref="contractServicesWrite" minOccurs="0"/>
				<xs:element ref="fafNumberWrite" minOccurs="0"/>
				<xs:element ref="serviceResources" minOccurs="0"/>
				<xs:element ref="contractWrite" minOccurs="0"/>
				<xs:element ref="contractInfoWrite" minOccurs="0"/>
				<xs:element ref="billingAccountAssignmentWrite" minOccurs="0"/>
				<xs:element ref="serviceParameters" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="customerRead">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="csId" type="xs:long" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Private BSCS customer ID.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="csIdPub" minOccurs="0"/>
				<xs:element ref="VPNCommunityID" minOccurs="0"/>
				<xs:element ref="VPNShortCode" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="contractNew">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="csIdPub" minOccurs="0"/>
				<xs:element ref="contractTypeIdPub" minOccurs="0"/>
				<xs:element ref="sccodePub" minOccurs="0"/>
				<xs:element ref="submIdPub" minOccurs="0"/>
				<xs:element ref="plcodePub" minOccurs="0"/>
				<xs:element ref="coArchive" minOccurs="0"/>
				<xs:element ref="coOpDir" minOccurs="0"/>
				<xs:element ref="coPstnDir" minOccurs="0"/>
				<xs:element ref="rpcodePub" minOccurs="0"/>
				<xs:element ref="coNonexplServPaymentType" minOccurs="0"/>
				<xs:element ref="currencyIdPub" minOccurs="0"/>
				<xs:element ref="callDetail" minOccurs="0"/>
				<xs:element ref="bmIdPub" minOccurs="0"/>
				<xs:element ref="chargingPeriodAdjustmentReference" minOccurs="0"/>
				<xs:element ref="ivrLangIdPub" minOccurs="0"/>
				<xs:element ref="sdpId" minOccurs="0"/>
				<xs:element ref="autoAssignCoreServ" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="contractDeviceAdd">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="resType" minOccurs="0"/>
				<xs:element ref="resId" minOccurs="0"/>
				<xs:element ref="resNum" minOccurs="0"/>
				<xs:element ref="smcId" minOccurs="0"/>
				<xs:element ref="hlcodePub" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="contractServicesAdd">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="services" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="serviceResources">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="item" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="contractWrite">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="coArchive" minOccurs="0"/>
				<xs:element ref="coOpDir" minOccurs="0"/>
				<xs:element ref="coPstnDir" minOccurs="0"/>
				<xs:element ref="coStatus" minOccurs="0"/>
				<xs:element ref="reason" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="csIdPub" type="xs:string">
		<xs:annotation>
			<xs:documentation>Public BSCS customer ID</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="contractTypeIdPub" type="xs:string">
		<xs:annotation>
			<xs:documentation>contract type public key</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sccodePub" type="xs:string">
		<xs:annotation>
			<xs:documentation>Public key of the market</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="submIdPub" type="xs:string">
		<xs:annotation>
			<xs:documentation>Public key of the submarket</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="plcodePub" type="xs:string">
		<xs:annotation>
			<xs:documentation>Public key of the network</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="coArchive" type="xs:string">
		<xs:annotation>
			<xs:documentation>Archive calls</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="coOpDir" type="xs:string">
		<xs:annotation>
			<xs:documentation>List in operator's directory</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="coPstnDir" type="xs:string">
		<xs:annotation>
			<xs:documentation>List in PSTN directory</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="rpcodePub" type="xs:string">
		<xs:annotation>
			<xs:documentation>Public key of the rateplan</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="coNonexplServPaymentType" type="xs:string">
		<xs:annotation>
			<xs:documentation>Indicate the payment type for the non-explicit contracted services.
Possible values:'R' : Prepaid ,'O' : Postpaid, 'D' - not specified: as in rateplan definition</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="currencyIdPub" type="xs:string">
		<xs:annotation>
			<xs:documentation>Public key of the currency</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="callDetail" type="xs:string">
		<xs:annotation>
			<xs:documentation>Indicates if individual calls should be displayed on call detail statement.  Domains:
R - on request, i.e. within requests generated for customers
P - in addition periodically within billing cycle related call detail statement generation requests
NULL - no call detail statement is generated</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bmIdPub" type="xs:string">
		<xs:annotation>
			<xs:documentation>Bill medium public key</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="chargingPeriodAdjustmentReference" type="xs:string">
		<xs:annotation>
			<xs:documentation>Charging period adjustment period for services with charging schedule set to "Cut off date related". Domain: 'B' - Billing period reference date (default), 'A' - Contract activation date</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ivrLangIdPub" type="xs:string">
		<xs:annotation>
			<xs:documentation>Public key of the language</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="resType" type="xs:string">
		<xs:annotation>
			<xs:documentation>Type of the device to be added
1 -Port ,2 - Storage medium	
Mandatory if &lt;code&gt;RES_ID&lt;/code&gt; or &lt;code&gt;RES_NUM&lt;/code&gt; are set.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="resId" type="xs:string">
		<xs:annotation>
			<xs:documentation>Id of the device to be added.
The table and column are dependent on the resource type &lt;code&gt;RES_TYPE&lt;/code&gt;:
Port - PORT.PORT_ID, Storage medium - STORAGE_MEDIUM.SM_ID</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="resNum" type="xs:string">
		<xs:annotation>
			<xs:documentation>New device number.Depending on the &lt;code&gt;RES_TYPE&lt;/code&gt; this parameter is either the storage medium number or a port number. The value is ignored if &lt;code&gt;RES_ID&lt;/code&gt; is set. Otherwise the value is used to determine the related resource.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="smcId" type="xs:string">
		<xs:annotation>
			<xs:documentation>Storage medium class id</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="services">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="items" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="items">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="spcodePub" minOccurs="0"/>
				<xs:element ref="sncodePub" minOccurs="0"/>
				<xs:element ref="profileId" minOccurs="0"/>
				<xs:element ref="isCore" minOccurs="0"/>
				<xs:element ref="OfferType" minOccurs="0"/>
				<xs:element ref="OfferSubType" minOccurs="0"/>
				<xs:element ref="Parameters" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="spcodePub" type="xs:string">
		<xs:annotation>
			<xs:documentation>Public key of the service package</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sncodePub" type="xs:string">
		<xs:annotation>
			<xs:documentation>Public key of the service</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="profileId" type="xs:string"/>
	<xs:element name="item">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="contractServiceResourcesWrite" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="contractServiceResourcesWrite">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="sncodePub" minOccurs="0"/>
				<xs:element ref="profileId" minOccurs="0"/>
				<xs:element ref="directoryNumberType" minOccurs="0"/>
				<xs:element ref="directoryNumbers" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="directoryNumbers">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="item" type="directoryNumbers" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="directoryNumbers">
		<xs:sequence>
			<xs:element name="dnId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Directory number id.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="mainDirnum" minOccurs="0"/>
			<xs:element ref="dirnumOnBill" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="mainDirnum" type="xs:string">
		<xs:annotation>
			<xs:documentation>Main directory number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dirnumOnBill" type="xs:string">
		<xs:annotation>
			<xs:documentation>DIRNUM_ON_BILL indicates  if the directory number is marked as shown on bill</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fafNumberWrite">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FAFNumberWriteInput" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FAFNumberWriteInput">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fafInformations" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="fafInformations">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="FAFInformation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FAFInformation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="fafNumber" minOccurs="0"/>
				<xs:element ref="numberingPlan" minOccurs="0"/>
				<xs:element ref="fafIndicator" minOccurs="0"/>
				<xs:element ref="fafAction" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="fafNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>The FAF number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="numberingPlan">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="id" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="id" type="xs:string">
		<xs:annotation>
			<xs:documentation>Internal identification key for FAF</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fafIndicator" type="xs:string">
		<xs:annotation>
			<xs:documentation>The FAF indicator for differentiated rating for traffic events to and from numbers in the FAF number list. The value range is 1 to 65535.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fafAction" type="xs:string">
		<xs:annotation>
			<xs:documentation>The FAF action to be performed for the FAF_NUMBER.
Possible values are: A = Add entry to the FaF list. U = Update elements of an entry in the FaF list. D = Delete an entry of the FaF list.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="coStatus" type="xs:string">
		<xs:annotation>
			<xs:documentation>New status for the contract. activation = 2, suspension = 3, deactive   = 4</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="reason" type="xs:string">
		<xs:annotation>
			<xs:documentation>Reason for the new Status. The reason must fit to the new status, delivered in &lt;code&gt;REASONS.READ RS_STATUS&lt;/code&gt; where the contract status maps to the following &lt;code&gt;RS_STATUS&lt;/code&gt; values: 1 -&gt; o (onhold), 2 -&gt; a (activation), 2 -&gt; r (reactivation) ,3 -&gt; s (suspension) ,4 -&gt; d (deactivation)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="orderHeader">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="correlationID" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>OM correlation ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="serviceOrderId" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>OM order ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="salesOrderId" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Siebel Order Id</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="buIdPubKey" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Business Unit Public Key</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="buIdPubValue" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Business Unit Public Value</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="sdpId" type="xs:int">
		<xs:annotation>
			<xs:documentation>SDP Id, only for Charging sytem integration. Can be changed only for onhold contracts.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="autoAssignCoreServ" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>Automatical assignment of core services</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="hlcodePub" type="xs:string">
		<xs:annotation>
			<xs:documentation>Public key of the physical HLR</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="isCore" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>FLAG to identify CORE services from OM</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="contractInfoWrite">
		<xs:complexType>
			<xs:all>
				<xs:annotation>
					<xs:documentation>
							&lt;p&gt;Identifies a contract with the public or private key.&lt;/p&gt;&lt;p&gt;Mandatory if no other contract-related command containing this information has been executed before.&lt;/p&gt;
						</xs:documentation>
				</xs:annotation>
				<xs:element name="text01" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Sales Agent</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text02" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Shop Name</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text03" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Sales Channel</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text04" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Alternate Notification number for data connection</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text05" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Subscriber Name</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text06" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Preferred Timing Start</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text07" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Preferred Timing End </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text08" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Type(Prepaid/Postpaid)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text09" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>IVR language Code</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text10" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								&lt;p&gt;identifies the text value&lt;/p&gt;
							</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text11" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								&lt;p&gt;identifies the text value&lt;/p&gt;
							</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text12" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								&lt;p&gt; identifies the text value &lt;/p&gt;
							</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text13" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								&lt;p&gt; identifies the text value &lt;/p&gt;
							</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="text14" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								&lt;p&gt; identifies the text value &lt;/p&gt;
							</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="creditExemptionFlag" type="xs:boolean" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								credit control exemption flag
							</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="collectionExemptionFlag" type="xs:boolean" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								dunning exemption flag
							</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="contractServicesWrite">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="services" type="servicesRequest" minOccurs="0"/>
				<xs:element name="instanceId" type="xs:long" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								Identifier of the instance</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="instanceIdPub" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								Public key of the instance</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="requestIdPub" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								Request public key that is unique among all MVNE/MVNO instances</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="effectiveDate" type="xs:dateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								Effective date of the MVNO request</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="coId" type="xs:long" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								Contract id</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="coIdPub" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								Public key of the contract</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="recordId" type="xs:long" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								Call record identifier, references the call event from the RTX table.	This argument (together with the RECORD_SUB_ID) triggers generation of the parent charging GMD request</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="recordSubId" type="xs:long" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
								Call record sub-identifier, references to the call event from the RTX table</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="servicesRequest">
		<xs:sequence>
			<xs:element name="item" type="servicesListpartRequest" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="servicesListpartRequest">
		<xs:sequence>
			<xs:element name="sncode" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
								Service code (ProductElement)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sncodePub" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
								Public key of the service</xs:documentation>
