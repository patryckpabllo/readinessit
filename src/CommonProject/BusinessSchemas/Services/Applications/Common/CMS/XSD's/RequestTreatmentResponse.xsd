							<xs:element name="EnglishOfferDescription" type="xs:string" minOccurs="0"/>
							<xs:element name="OfferZKB" type="xs:string" minOccurs="0"/>
							<xs:element name="campaignGroupId" type="xs:string" minOccurs="0"/>
