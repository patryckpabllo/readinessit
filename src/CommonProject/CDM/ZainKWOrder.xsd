<?xml version="1.0" encoding="ISO-8859-1"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns:cdm="http://www.ericsson.com/CDM/telecom/canonicaldatamodel"
	 targetNamespace="http://www.ericsson.com/CDM/telecom/canonicaldatamodel"
	 version="2.2"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">

	<!-- The type: com.sid.CommonBusinessEntitiesDomain.BusinessInteractionItem -->
	<!-- The type: com.sid.CommonBusinessEntitiesDomain.BusinessInteractionItemPrice -->
	<!-- The type: com.sid.CommonBusinessEntitiesDomain.BusinessInteractionLocation -->
	<!-- The type: com.sid.CommonBusinessEntitiesDomain.BusinessInteractionRelationship -->
	<!-- The type: com.sid.CommonBusinessEntitiesDomain.BusinessInteractionRole -->
	<!-- The type: com.sid.CommonBusinessEntitiesDomain.BusinessInteractionSpec -->
	<!-- The type: com.sid.CommonBusinessEntitiesDomain.BusinessInteractionType -->
	<!-- The type: com.sid.CommonBusinessEntitiesDomain.BusinessInteractionVersion -->
	<!-- RESOURCE-->
	<xs:element name="PaymentMethod" type="cdm:PaymentMethod"/>
	<xs:complexType name="PaymentMethod">
		<xs:annotation>
			<xs:documentation>PaymentMethod is the way in which customer provides payment, for
                example cash ,credit,transfer.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="description" type="xs:string" minOccurs="0"/>
			<xs:element name="ID" type="xs:string" minOccurs="0"/>
			<xs:element name="name" type="xs:string" minOccurs="0"/>
			<xs:element name="paymentMethodType" type="xs:string" minOccurs="0"/>
			<xs:element name="CreditCardPM" type="cdm:CreditCardPM" minOccurs="0"/>
			<xs:element name="CashPM" type="cdm:CashPM" minOccurs="0"/>
			<xs:element name="ZainBankPM" type="cdm:ZainBankPM" minOccurs="0"/>
			<xs:element name="PreferredPaymentMode" type="xs:string" minOccurs="0"/>
			<xs:element ref="cdm:ChequeDetails" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Agreement" type="cdm:Agreement"/>
	<xs:complexType name="Agreement">
		<xs:annotation>
			<xs:documentation>A type of BusinessInteraction that represents a contract or
                arrangement, either written or verbal and sometimes enforceable by
                law.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="agreementName" type="xs:string"/>
			<xs:element name="agreementID" type="xs:string"/>
			<xs:element name="agreementType" type="xs:string" minOccurs="0"/>
			<xs:element name="agreementStatementOfIntent" type="xs:string">
				<xs:annotation>
					<xs:documentation>An overview and goals of the
                                Agreement.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="agreementItem" type="xs:string"/>
			<xs:element name="agreementStatus" type="xs:string" minOccurs="0"/>
			<xs:element name="agreementDates" type="cdm:TimePeriod" minOccurs="0"/>
			<xs:element name="penaltyValue" type="cdm:Money" minOccurs="0"/>
			<xs:element name="commitValue" type="xs:int" minOccurs="0"/>
			<xs:element name="message" type="xs:string" minOccurs="0"/>
			<xs:element name="minCommitmentThreshold" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ContactMedium" type="cdm:ContactMedium"/>
	<xs:complexType name="ContactMedium" abstract="true">
		<xs:annotation>
			<xs:documentation>A means by which communication may be established with a party
                (individual or organization).Contact Medium is an abstract concept that should be
                subclassed as required.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ValidityPeriod" type="cdm:TimePeriod" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CustomerBill" type="cdm:CustomerBill"/>
	<xs:complexType name="CustomerBill">
		<xs:annotation>
			<xs:documentation>A total amount due from a CustomerAccount for all products during the
                billing period.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="billDate" type="xs:int" minOccurs="0"/>
			<xs:element name="billNo" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identification number of a bill.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="cdm:billId" minOccurs="0"/>
			<xs:element ref="cdm:addressId" minOccurs="0"/>
			<xs:element name="Format" type="xs:string" minOccurs="0"/>
			<xs:element name="chargeDate" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A date through which charges previously received by the
                        billing system appear on the bill.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creditDate" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A date through which credits previously received by the
                        billing system appear on the bill.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creditLimit" type="xs:string" minOccurs="0"/>
			<xs:element name="mailingDate" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A customer bill mailing date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentDueDate" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A customer bill payment due date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="relationshipType" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Defines the type of connection between two
                        CustomerAccounts.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomerBillingCycle" type="cdm:CustomerBillingCycle" minOccurs="0"/>
			<xs:element name="CustomerBillFormat" type="cdm:CustomerBillFormat" minOccurs="0"/>
			<xs:element name="Money" type="cdm:Money" minOccurs="0"/>
			<xs:element ref="cdm:ZainContact" minOccurs="0"/>
			<xs:element name="numberOfCopies" type="xs:int" minOccurs="0"/>
			<xs:element name="nameToPrint" type="xs:string" minOccurs="0"/>
			<xs:element name="RequestType" type="xs:string" minOccurs="0"/>
			<xs:element name="RequestsubType" type="xs:string" minOccurs="0"/>
			<xs:element name="ContractTypeId" type="xs:string" minOccurs="0"/>
			<xs:element name="Information" type="xs:boolean" minOccurs="0"/>
			<xs:element name="simulation" type="xs:boolean" minOccurs="0"/>
			<xs:element name="RefernceDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="PostingPeriod" type="xs:string" minOccurs="0"/>
			<xs:element name="acknowledgment" type="xs:boolean" minOccurs="0"/>
			<xs:element name="CollectInd" type="xs:boolean" minOccurs="0"/>
			<xs:element name="Ignorebuind" type="xs:boolean" minOccurs="0"/>
			<xs:element name="allbusinesunits" type="xs:boolean" minOccurs="0"/>
			<xs:element name="disableinvoiceforwarding" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CreditCardPM" type="cdm:CreditCardPM"/>
	<xs:complexType name="CreditCardPM">
		<xs:annotation>
			<xs:documentation>CreditCardPM is an example of&lt;span style="mso-spacerun:
                yes"&gt;&lt;/span&gt; payment method, using which payment can be made by credit
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="creditCardNumber" type="xs:string" minOccurs="0"/>
			<xs:element name="CreditCardCompanyName" type="xs:string" minOccurs="0"/>
			<xs:element name="expirationDate" type="xs:date" minOccurs="0"/>
			<xs:element name="ExpirationMonth" type="xs:string" minOccurs="0"/>
			<xs:element name="ExpirationYear" type="xs:string" minOccurs="0"/>
			<xs:element name="nameOnCard" type="xs:string" minOccurs="0"/>
			<xs:element name="authorizationCode" type="xs:string" minOccurs="0"/>
			<xs:element name="creditCardType" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Party" type="cdm:Party"/>
	<xs:complexType name="Party" abstract="true">
		<xs:annotation>
			<xs:documentation>Represents an individual, organization or organization unit.Party is
                an abstract concept that should be used in places where the business says something
                can be an organization , organization unit or an individual</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="partyId" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unique identifier for Party</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validFor" type="cdm:TimePeriod" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="LogicalResource" type="cdm:LogicalResource"/>
	<xs:complexType name="LogicalResource">
		<xs:annotation>
			<xs:documentation>Resource information related to a CustomerOrder are
                stored.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="resourceName" type="xs:string" minOccurs="0"/>
			<xs:element name="resourceValue" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CustomerOrder" type="cdm:CustomerOrder"/>
	<xs:complexType name="CustomerOrder" abstract="true">
		<xs:annotation>
			<xs:documentation>A CustomerOrder is a communication used to procure a Product. There
                are different types of requests, such as AccessServiceRequests (ASRs),
                LocalServiceRequests (LSRs), DirectoryServiceRequests (DSRs), ProductOrders
                (PSRs).The abstract Order object provides a generalization of
                relationships.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="orderChannel" type="xs:string" minOccurs="0"/>
			<xs:element ref="cdm:orderId" minOccurs="0"/>
			<xs:element name="customerOrderType" type="xs:string" minOccurs="0"/>
			<xs:element name="customerOrderSubType" type="xs:string" minOccurs="0"/>
			<xs:element name="creationDateTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="dueDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date upon which the CustomerOrder is requested to be provisioned</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderFulfilmentDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Order completion date</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="assignedPriority" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The order's assigned priority after review.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="cdm:SalesChannel" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ZainCustomerOrder" type="cdm:ZainCustomerOrder"/>
	<xs:complexType name="ZainCustomerOrder">
		<xs:annotation>
			<xs:documentation>A CustomerOrder is a communication used to procure a Product. There are different types of requests, such as AccessServiceRequests (ASRs),LocalServiceRequests (LSRs), DirectoryServiceRequests (DSRs), ProductOrders (PSRs).The abstract Order object provides a generalization of relationships.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cdm:CustomerOrder">
				<xs:sequence>
					<xs:element name="CustomerInfo" type="cdm:ZainIndividual"/>
					<xs:element ref="cdm:BillingContract" minOccurs="0"/>
					<xs:element name="totalNRCAmount" type="xs:double" minOccurs="0"/>
					<xs:element name="totalMRCAmount" type="xs:double" minOccurs="0"/>
					<xs:element name="totalNRCUpfront" type="xs:double" minOccurs="0"/>
					<xs:element name="totalRCAdvance" type="xs:double" minOccurs="0"/>
					<xs:element name="totalDepositAmount" type="xs:double" minOccurs="0"/>
					<xs:element name="ZainCurrency" type="cdm:ZainCurrency" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="orderReceiptNumber" type="xs:string" minOccurs="0"/>
					<xs:element name="orderReason" type="xs:string" minOccurs="0"/>
					<xs:element name="batchDetails" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="name" type="xs:string"/>
								<xs:element ref="cdm:value"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element ref="cdm:Agreement" minOccurs="0"/>
					<xs:element name="escalationFlag" type="xs:boolean" minOccurs="0"/>
					<xs:element name="estimationTime" type="xs:string" minOccurs="0"/>
					<xs:element name="revision" type="xs:string" minOccurs="0"/>
					<xs:element name="comment" type="xs:string" minOccurs="0"/>
					<xs:element name="ZainCustomerOrderItem" type="cdm:ZainCustomerOrderItem" maxOccurs="unbounded"/>
					<xs:element ref="cdm:ZainCustomerPayment" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="CustomerPayment" type="cdm:CustomerPayment"/>
	<xs:complexType name="CustomerPayment">
		<xs:annotation>
			<xs:documentation>CustomerPayment represents the transfer of wealth from a customer for
                a product or usage of product.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="cdm:Money" minOccurs="0"/>
			<xs:element ref="cdm:conversionRate" minOccurs="0"/>
			<xs:element name="TransactionID" type="xs:string" minOccurs="0"/>
			<xs:element name="paymentDate" type="xs:date" minOccurs="0"/>
			<xs:element name="paymentReceiptDate" type="xs:date" minOccurs="0"/>
			<xs:element name="paymentType" type="xs:string" minOccurs="0"/>
			<xs:element ref="cdm:PaymentMethod" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="AgreementItem" type="cdm:AgreementItem"/>
	<xs:complexType name="AgreementItem">
		<xs:annotation>
			<xs:documentation>The purpose for an Agreement expressed in terms of a Product, Service,
                Resource, and/or their respective specifications, inherited from
                BusinessInteractionItem.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cdm:CustomerOrderItem"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="IndividualIdentification" type="cdm:IndividualIdentification"/>
	<xs:complexType name="IndividualIdentification" abstract="true">
		<xs:annotation>
			<xs:documentation>Individual Identification represents our registration of information
                used as proof of identity by an Individual</xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<xs:element name="TimePeriod" type="cdm:TimePeriod"/>
	<xs:complexType name="TimePeriod">
		<xs:annotation>
			<xs:documentation>A base / value business entity used to represent a period of time,
                between two timepoints</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="endDateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An instant of time, ending at the
                        TimePeriod</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="startDateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An instant of time, starting at the
                        TimePeriod</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="TelephoneNumber" type="cdm:TelephoneNumber"/>
	<xs:complexType name="TelephoneNumber">
		<xs:complexContent>
			<xs:extension base="cdm:ContactMedium">
				<xs:sequence>
					<xs:element name="number" type="xs:string" minOccurs="0"/>
					<xs:element name="type" type="xs:int" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="SocialSecurityNrIdentification" type="cdm:SocialSecurityNrIdentification"/>
	<xs:complexType name="SocialSecurityNrIdentification">
		<xs:complexContent>
			<xs:extension base="cdm:IndividualIdentification">
				<xs:sequence>
					<xs:element name="socialSecurityNr" type="xs:int"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="CashPM" type="cdm:CashPM"/>
	<xs:complexType name="CashPM">
		<xs:annotation>
			<xs:documentation>payment method, through which payment can be made by
                cash.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cashierInfo" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Individual" type="cdm:Individual"/>
	<xs:complexType name="Individual">
		<xs:complexContent>
			<xs:extension base="cdm:Party">
				<xs:sequence>
					<xs:element name="disabilities" type="xs:int" minOccurs="0"/>
					<xs:element name="gender" type="xs:string" minOccurs="0"/>
					<xs:element name="languageAbility" type="xs:int" minOccurs="0"/>
					<xs:element name="maritalStatus" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Holds all the information of an
                                individual.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="nationality" type="xs:string" minOccurs="0"/>
					<xs:element name="placeOfBirth" type="xs:string" minOccurs="0"/>
					<xs:element name="skills" type="xs:string" minOccurs="0"/>
					<xs:element name="profession" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="EmailContact" type="cdm:EmailContact"/>
	<xs:complexType name="EmailContact">
		<xs:annotation>
			<xs:documentation>The eMail address to use when contacting a given PartyRole. An
                eMailContact is a type of ContactMedium.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cdm:ContactMedium">
				<xs:sequence>
					<xs:element name="eMailAddress" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>full e-mail address in standard
                                format</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="CustomerOrderItem" type="cdm:CustomerOrderItem"/>
	<xs:complexType name="CustomerOrderItem">
		<xs:annotation>
			<xs:documentation>The purpose for the Customer Order expressed in terms of a Product
                Offering or a Product .</xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<xs:element name="MarketSegment" type="cdm:MarketSegment"/>
	<xs:complexType name="MarketSegment" abstract="true">
		<xs:annotation>
			<xs:documentation>A grouping of Parties, GeographicAreas, SalesChannels, and so forth.
                MarketSegments are the target of MarketingCampaigns, ProductOfferings,
                ProductPromotions, ProductPlacements, and ProductPrograms from both internal and
                external (Competitors, and other Providers) perspective.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="description" type="xs:string">
				<xs:annotation>
					<xs:documentation>A narrative that explains what the MarketSegment
                        is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ID" type="xs:string">
				<xs:annotation>
					<xs:documentation>A unique identifier for the MarketSegment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="marketSegmentSubCategory" type="xs:string">
				<xs:annotation>
					<xs:documentation>A further classification of MarketSegments because they share
                        common characteristics. For example, a vertical category MarketSegment may
                        be further categorized as consumer, business, or carrier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:string">
				<xs:annotation>
					<xs:documentation>A word, term, or phrase by which a MarketSegment is known and
                        distinguished from other MarketSegments.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validFor" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The period during which the MarketSegment is
                        applicable.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ZainOrderHeader" type="cdm:ZainOrderHeader"/>
	<xs:complexType name="ZainOrderHeader">
		<xs:sequence>
			<xs:element name="businessMessageID" type="xs:string" minOccurs="0"/>
			<xs:element name="businessMessageName" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CustomerBillFormat" type="cdm:CustomerBillFormat"/>
	<xs:complexType name="CustomerBillFormat">
		<xs:annotation>
			<xs:documentation>A detailed description of the way in which a customer's bill is
                presented. The current version of this entity serves as a placeholder. It will be
                further developed in a future version of this document.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="description" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A description of the bill format.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A short descriptive name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hasCallDetails" type="xs:string" minOccurs="0"/>
			<xs:element name="CustomerBillPresentationMedia" type="cdm:CustomerBillPresentationMedia"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Money" type="cdm:Money"/>
	<xs:complexType name="Money">
		<xs:annotation>
			<xs:documentation>A base / value business entity used to represent
                money</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="amount" type="xs:double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A positive floating point number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="units" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Currency.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ZainCustomerOrderItem" type="cdm:ZainCustomerOrderItem"/>
	<xs:complexType name="ZainCustomerOrderItem">
		<xs:annotation>
			<xs:documentation>The purpose for the Customer Order expressed in terms of a Product
                Offering or a Product.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cdm:CustomerOrderItem">
				<xs:sequence>
					<xs:element name="actionCode" type="xs:string" minOccurs="0"/>
					<xs:element name="CRMIntegrationID" type="xs:string" minOccurs="0"/>
					<xs:element name="reasonCode" type="xs:string" minOccurs="0"/>
					<xs:element name="refundFlag" type="xs:string" minOccurs="0"/>
					<xs:element name="upfrontFlag" type="xs:string" minOccurs="0"/>
					<xs:element name="isPriceOverride" type="xs:string" minOccurs="0"/>
					<xs:element name="ListOfServiceParameters" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ServiceParameters" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="ActionCode" type="xs:string" minOccurs="0"/>
											<xs:element name="Name" type="xs:string" minOccurs="0"/>
											<xs:element name="Value" type="xs:string" minOccurs="0"/>
											<xs:element name="OldValue" type="xs:string" minOccurs="0"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
