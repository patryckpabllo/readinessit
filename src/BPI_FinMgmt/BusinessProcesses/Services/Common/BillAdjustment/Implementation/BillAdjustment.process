    <pd:activity name="Call_FinancialTransactionWrite">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>521</pd:x>
        <pd:y>121</pd:y>
        <config>
            <processName>/BusinessProcesses/Services/Common/BillAdjustment/HelperProcess/ESI_BSCS_Call_FinancialTransactionWrite.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <ns25:financialTransactionWriteRequest>
                    <ns25:inputAttributes>
                        <xsl:if test="$Parse-XML/pfx11:BillAdjustmentRequest/ns19:Header/ns19:ExternalTransactionID">
                            <ns25:transactionRefnum>
                                <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/ns19:Header/ns19:ExternalTransactionID"/>
                            </ns25:transactionRefnum>
                        </xsl:if>
                        <xsl:if test="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:GLCode">
                            <ns25:glAccount>
                                <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:GLCode"/>
                            </ns25:glAccount>
                        </xsl:if>
                        <ns25:useCaseCode>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/BusinessProcesses/Services/Common/BillAdjustment/PaymentType"/>
                        </ns25:useCaseCode>
                        <ns25:amountCashPay>
                            <pfx9:amount>
                                <xsl:value-of select="if (upper-case($Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:Adjustment/ns20:CreditOrDebit)=$_globalVariables/ns1:GlobalVariables/BusinessProcesses/Services/Common/BillAdjustment/AdjType)&#xA;then -$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:Adjustment/ns20:AdjustmentAmount&#xA;else $Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:Adjustment/ns20:AdjustmentAmount"/>
                            </pfx9:amount>
                            <pfx9:currency>
                                <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/BusinessProcesses/Services/Common/BillAdjustment/defaultCurrencyCode"/>
                            </pfx9:currency>
                        </ns25:amountCashPay>
                        <xsl:if test="string-length($Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:Identifier/pfx11:BillingAccountID)>0&#xA;or&#xA;string-length($Call_MSISDN_BA_Read/ns23:QueryResponse/ns19:billId/ns19:value)>0">
                            <xsl:choose>
                                <xsl:when test="string-length($Call_MSISDN_BA_Read/ns23:QueryResponse/ns19:billId/ns19:value)>0">
                                    <ns25:billingAccountId>
                                        <xsl:value-of select="$Call_MSISDN_BA_Read/ns23:QueryResponse/ns19:billId/ns19:value"/>
                                    </ns25:billingAccountId>
                                </xsl:when>
                                <xsl:otherwise>
                                    <ns25:billingAccountId>
                                        <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:Identifier/pfx11:BillingAccountID"/>
                                    </ns25:billingAccountId>
                                </xsl:otherwise>
                            </xsl:choose>
                        </xsl:if>
                        <xsl:choose>
                            <xsl:when test="string-length($Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:Identifier/pfx11:CustomerID)>0">
                                <xsl:if test="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:Identifier/pfx11:CustomerID">
                                    <ns25:csIdPub>
                                        <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:Identifier/pfx11:CustomerID"/>
                                    </ns25:csIdPub>
                                </xsl:if>
                            </xsl:when>
                            <xsl:otherwise>
                                <ns25:csIdPub>
                                    <xsl:value-of select="$Call_MSISDN_BA_Read/ns23:QueryResponse/ns19:customerId/ns19:value"/>
                                </ns25:csIdPub>
                            </xsl:otherwise>
                        </xsl:choose>
                        <xsl:if test="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:remark">
                            <ns25:remark>
                                <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:remark"/>
                            </ns25:remark>
                        </xsl:if>
                        <xsl:if test="string-length($Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:Adjustment/ns19:InvoiceNumber)>0">
                            <ns25:details>
                                <ns25:item>
                                    <ns25:documentId>
                                        <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:Adjustment/ns19:InvoiceNumber {-- Invoice ID to be mapped --}"/>
                                    </ns25:documentId>
                                    <ns25:creditDebit>
                                        <xsl:value-of select="&quot;C&quot;"/>
                                    </ns25:creditDebit>
                                    <ns25:assAmountCashPay>
                                        <pfx9:amount>
                                            <xsl:value-of select="if (upper-case($Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:Adjustment/ns20:CreditOrDebit)=$_globalVariables/ns1:GlobalVariables/BusinessProcesses/Services/Common/BillAdjustment/AdjType)&#xA;then -$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:Adjustment/ns20:AdjustmentAmount&#xA;else $Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:Adjustment/ns20:AdjustmentAmount"/>
                                        </pfx9:amount>
                                        <pfx9:currency>
                                            <xsl:value-of select="&quot;KWD&quot;"/>
                                        </pfx9:currency>
                                    </ns25:assAmountCashPay>
                                </ns25:item>
                            </ns25:details>
                        </xsl:if>
                    </ns25:inputAttributes>
                </ns25:financialTransactionWriteRequest>
                <xsl:if test="$BillAdjustmentReceiver/ns3:ActivityOutput/OtherProperties/TransactionID">
                    <TrackingId>
                        <xsl:value-of select="$BillAdjustmentReceiver/ns3:ActivityOutput/OtherProperties/TransactionID"/>
                    </TrackingId>
                </xsl:if>
                <StartTimeStamp>
                    <xsl:value-of select="if(exists($BillAdjustmentReceiver/ns3:ActivityOutput/OtherProperties/StartTimeStamp) and string-length($BillAdjustmentReceiver/ns3:ActivityOutput/OtherProperties/StartTimeStamp) > 0)&#xA;then $BillAdjustmentReceiver/ns3:ActivityOutput/OtherProperties/StartTimeStamp&#xA;else 0"/>
                </StartTimeStamp>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Call_ExtPaymentDetailsWrite">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>634</pd:x>
        <pd:y>122</pd:y>
        <config>
            <processName>/BusinessProcesses/Services/Common/BillAdjustment/HelperProcess/ESI_BSCS_Call_ExtPaymentDetailsWrite.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <pfx12:extPaymentDetailsWriteRequest>
                    <pfx12:inputAttributes>
                        <pfx12:caxact>
                            <xsl:value-of select="$Call_FinancialTransactionWrite/ns25:financialTransactionWriteResponse/ns25:resultList/ns25:item[1]/ns25:transactionId"/>
                        </pfx12:caxact>
                        <pfx12:transactionRefnum>
                            <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/ns19:Header/ns19:ExternalTransactionID"/>
                        </pfx12:transactionRefnum>
                        <pfx12:paymentAmount>
                            <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:Adjustment/ns20:AdjustmentAmount"/>
                        </pfx12:paymentAmount>
                        <pfx12:payMethod>
                            <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:modPay"/>
                        </pfx12:payMethod>
                        <pfx12:payAuthCode>
                            <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:PaymentMethod/ns20:CreditCardPM/ns20:authorizationCode"/>
                        </pfx12:payAuthCode>
                        <pfx12:channel>
                            <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:payDesk"/>
                        </pfx12:channel>
                        <pfx12:user>
                            <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:user"/>
                        </pfx12:user>
                        <xsl:if test="string-length($Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:SourceDetails/ns20:merchantId)>0">
                            <pfx12:merchantId>
                                <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:SourceDetails/ns20:merchantId"/>
                            </pfx12:merchantId>
                        </xsl:if>
                        <xsl:if test="string-length($Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:SourceDetails/ns20:terminalId)>0">
                            <pfx12:terminalId>
                                <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:SourceDetails/ns20:terminalId"/>
                            </pfx12:terminalId>
                        </xsl:if>
                        <pfx12:customerCategory>
                            <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:References/pfx11:items[1]/pfx11:Value"/>
                        </pfx12:customerCategory>
                        <pfx12:crcardno>
                            <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:PaymentMethod/ns20:CreditCardPM/ns20:creditCardNumber"/>
                        </pfx12:crcardno>
                        <xsl:if test="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:Adjustment/ns20:Description">
                            <pfx12:remarks>
                                <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/ns20:Adjustment/ns20:Description"/>
                            </pfx12:remarks>
                        </xsl:if>
                        <pfx12:sourceTransactionId>
                            <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/ns19:Header/ns19:ExternalTransactionID"/>
                        </pfx12:sourceTransactionId>
                        <pfx12:location>
                            <xsl:value-of select="$Parse-XML/pfx11:BillAdjustmentRequest/pfx11:BillAdjustment/pfx11:workArea"/>
                        </pfx12:location>
                    </pfx12:inputAttributes>
                </pfx12:extPaymentDetailsWriteRequest>
                <xsl:if test="$BillAdjustmentReceiver/ns3:ActivityOutput/OtherProperties/TransactionID">
                    <TrackingId>
                        <xsl:value-of select="$BillAdjustmentReceiver/ns3:ActivityOutput/OtherProperties/TransactionID"/>
                    </TrackingId>
                </xsl:if>
                <xsl:if test="$BillAdjustmentReceiver/ns3:ActivityOutput/OtherProperties/StartTimeStamp">
                    <StartTimeStamp>
                        <xsl:value-of select="$BillAdjustmentReceiver/ns3:ActivityOutput/OtherProperties/StartTimeStamp"/>
                    </StartTimeStamp>
                </xsl:if>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Reply to JMS Message">
        <pd:type>com.tibco.plugin.jms.JMSReplyActivity</pd:type>
        <pd:resourceType>ae.activities.JMSReplyActivity</pd:resourceType>
        <pd:x>930</pd:x>
        <pd:y>128</pd:y>
        <config>
            <PermittedMessageType>Text</PermittedMessageType>
