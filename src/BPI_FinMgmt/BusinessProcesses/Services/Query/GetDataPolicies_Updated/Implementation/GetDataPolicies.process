                                                            <xsl:value-of select="AssetStatus"/>
                                                        </ns34:AssetStatus>
                                                    </xsl:if>
                                                    <xsl:if test="$ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS">
                                                        <AssetStatusDescription>
                                                            <xsl:value-of select="$ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS"/>
                                                        </AssetStatusDescription>
                                                    </xsl:if>
                                                </ns34:Asset>
                                            </xsl:for-each>
                                        </xsl:if>
                                    </xsl:otherwise>
                                </xsl:choose>
                            </xsl:if>
                        </xsl:for-each>
                    </ns34:ListOfAssets>
                </ns34:SubscriberAssets>
            </ns33:DataInquiriesResponse>
        </pd:inputBindings>
