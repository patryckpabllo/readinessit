                                                                        <xsl:value-of select="ServiceID"/>
                                                                    </ns34:ServiceID>
                                                                    <ns34:ServiceType>
                                                                        <xsl:value-of select="$UCServiceID"/>
                                                                    </ns34:ServiceType>
                                                                    <ns34:ServiceVolume>
                                                                        <xsl:value-of select="if(($CurrentId=current()/ServiceID) and string-length(current()/ServiceVolume)>=0 and string-length($UCServiceID)>0 and $ProductId !=0)&#xA;then&#xA;0&#xA;else if(($CurrentId=current()/ServiceID) and string-length(current()/ServiceVolume)>=0 and string-length($UCServiceID)>0)&#xA;then&#xA;(      &#x9;&#x9; if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='GB')  then ( $UCServiceID)*(1div 1024)*(1div 1024)*(1div 1024)  else&#xA;&#x9;&#x9;if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='TB')  then ($UCServiceID)*(1div 1024)*(1div 1024)*(1div 1024)*(1div 1024) else&#xA;&#x9;&#x9;if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='MB')  then ($UCServiceID)*(1div 1024)*(1div 1024) else&#xA;&#x9;&#x9;if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='KB')  then ($UCServiceID)*(1div 1024) else&#xA;&#x9;&#x9;if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='Minutes')  then ($UCServiceID)*(1div 60) else&#xA;&#x9;&#x9;$UCServiceID)&#xA;else if(($CurrentId=current()/ServiceID) and string-length(current()/ServiceVolume)>0)&#xA;then&#xA;&#x9;      (if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='GB')  then ( current()/ServiceVolume)*(1div 1024)*(1div 1024)*(1div 1024)  else&#xA;&#x9;&#x9;if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='TB')  then (current()/ServiceVolume)*(1div 1024)*(1div 1024)*(1div 1024)*(1div 1024) else&#xA;&#x9;&#x9;if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='MB')  then (current()/ServiceVolume)*(1div 1024)*(1div 1024) else&#xA;&#x9;&#x9;if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='KB')  then (current()/ServiceVolume)*(1div 1024) else&#xA;&#x9;&#x9;if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='Minutes')  then (current()/ServiceVolume)*(1div 60) else&#xA;&#x9;&#x9;current()/ServiceVolume&#xA;)&#xA;else if(($CurrentId=current()/ServiceID) and string-length(current()/ServiceVolume)=0)&#xA;then&#xA;0&#xA;else&#xA;0"/>
                                                                    </ns34:ServiceVolume>
                                                                    <ns34:ServiceUnit>
                                                                        <xsl:value-of select="if($CurrentId=$Call_GetUCUT_Int/ns33:DataInquiriesResponse/ns34:SubscriberAssets/ns34:ListOfAssets/ns34:Asset/ns34:AssetID)&#xA;then (&#xA;&#x9;&#x9;if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='GB')  then ($Call_GetUCUT_Int/ns33:DataInquiriesResponse/ns34:SubscriberAssets/ns34:ListOfAssets/ns34:Asset/ListOfServices/ns34:Service[ns34:ServiceID=current()/ServiceID]/ns34:ServiceUnit)*(1div 1024)*(1div 1024)*(1div 1024)  else&#xA;                if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='TB')  then ($Call_GetUCUT_Int/ns33:DataInquiriesResponse/ns34:SubscriberAssets/ns34:ListOfAssets/ns34:Asset/ListOfServices/ns34:Service[ns34:ServiceID=current()/ServiceID]/ns34:ServiceUnit)*(1div 1024)*(1div 1024)*(1div 1024)*(1div 1024) else&#xA;                if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='MB')  then ($Call_GetUCUT_Int/ns33:DataInquiriesResponse/ns34:SubscriberAssets/ns34:ListOfAssets/ns34:Asset/ListOfServices/ns34:Service[ns34:ServiceID=current()/ServiceID]/ns34:ServiceUnit)*(1div 1024)*(1div 1024) else&#xA;                if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='KB')  then ($Call_GetUCUT_Int/ns33:DataInquiriesResponse/ns34:SubscriberAssets/ns34:ListOfAssets/ns34:Asset/ListOfServices/ns34:Service[ns34:ServiceID=current()/ServiceID]/ns34:ServiceUnit)*(1div 1024) else&#xA;                if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='Seconds')  then ($Call_GetUCUT_Int/ns33:DataInquiriesResponse/ns34:SubscriberAssets/ns34:ListOfAssets/ns34:Asset/ListOfServices/ns34:Service[ns34:ServiceID=current()/ServiceID]/ns34:ServiceUnit)  else&#xA;                $ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:THRESHOLD)&#xA;else&#xA;&#x9;(if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='GB')  then (current()/ServiceUnit)*(1div 1024)*(1div 1024)*(1div 1024)  else&#xA;                if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='TB')  then (current()/ServiceUnit)*(1div 1024)*(1div 1024)*(1div 1024)*(1div 1024) else&#xA;                if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='MB')  then (current()/ServiceUnit)*(1div 1024)*(1div 1024) else&#xA;                if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='KB')  then (current()/ServiceUnit)*(1div 1024) else&#xA;                if($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:UNITS='Seconds')  then (current()/ServiceUnit)  else&#xA;           $ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:THRESHOLD)"/>
                                                                    </ns34:ServiceUnit>
                                                                </ns34:Service>
                                                            </xsl:if>
                                                        </xsl:for-each>
                                                    </ListOfServices>
                                                    <xsl:for-each select="product">
                                                        <ns34:Product>
                                                            <xsl:if test="ProductName">
                                                                <ns34:ProductName>
                                                                    <xsl:value-of select="ProductName"/>
                                                                </ns34:ProductName>
                                                            </xsl:if>
                                                            <xsl:if test="ProductID">
                                                                <ns34:ProductID>
                                                                    <xsl:value-of select="ProductID"/>
                                                                </ns34:ProductID>
                                                            </xsl:if>
                                                        </ns34:Product>
                                                    </xsl:for-each>
                                                    <xsl:for-each select="Price">
                                                        <ns34:Price>
                                                            <xsl:if test="One-Time">
                                                                <ns34:One-Time>
                                                                    <xsl:value-of select="One-Time"/>
                                                                </ns34:One-Time>
                                                            </xsl:if>
                                                            <xsl:if test="Recurring">
                                                                <ns34:Recurring>
                                                                    <xsl:value-of select="Recurring"/>
                                                                </ns34:Recurring>
