                                                <xsl:variable name="Threshold" select="if(string-length($DateCalc)>0 and $DateCalc!=0 and $Units='Seconds')&#xA;then&#xA;(ceiling(((ns34:ServiceUnit div $Day)  * $DateCalc) div 60) * 60)&#xA;else if (string-length($DateCalc)>0 and $DateCalc!=0 and $Units = 'Count')&#xA;then&#xA;ceiling((ns34:ServiceUnit div $Day)  * $DateCalc)&#xA;else if (string-length($DateCalc)>0 and $DateCalc!=0 and $Units = 'Money')&#xA;then&#xA;tib:round-fraction(((ns34:ServiceUnit div $Day)  * $DateCalc),3)&#xA;else&#xA;ns34:ServiceUnit"/>
                                                <ns34:ServiceID>
                                                    <xsl:value-of select="ns34:ServiceID"/>
                                                </ns34:ServiceID>
                                                <ns34:ServiceType>
                                                    <xsl:value-of select="ns34:ServiceType"/>
                                                </ns34:ServiceType>
                                                <ns34:ServiceVolume>
                                                    <xsl:value-of select="if(($UNIT='Count'  or $UNIT='Money' ) and string-length(current()/ns34:ServiceVolume)>0)&#xA;then&#xA;$Threshold - current()/ns34:ServiceVolume&#xA;else if($UNIT='Seconds'  and string-length(current()/ns34:ServiceVolume)>0)&#xA;then&#xA;$Threshold -(current()/ns34:ServiceVolume *60) &#xA;else&#xA;$ESB_DAThreshold/ESB_DAThresholdMapping/Record[DA_ID=current()/ns34:ServiceID]/DA_Threshold div 60 - ($Threshold)&#xA;&#xA;{--Fetching the DA Details from the ESB_Threhsold DA Table.for the Postpaid DA's--}"/>
                                                </ns34:ServiceVolume>
                                                <ns34:ServiceUnit>
                                                    <xsl:value-of select="if(string-length($DateCalc)>0 and $DateCalc!=0 and $Units='Seconds')&#xA;then&#xA;(ceiling(((ns34:ServiceUnit div $Day)  * $DateCalc) div 60) * 60)&#xA;else if (string-length($DateCalc)>0 and $DateCalc!=0 and $Units = 'Count')&#xA;then&#xA;ceiling((ns34:ServiceUnit div $Day)  * $DateCalc)&#xA;else if (string-length($DateCalc)>0 and $DateCalc!=0 and $Units = 'Money')&#xA;then&#xA;tib:round-fraction(((ns34:ServiceUnit div $Day)  * $DateCalc),3)&#xA;else&#xA;ns34:ServiceUnit"/>
                                                </ns34:ServiceUnit>
                                            </ns34:Service>
                                        </xsl:for-each>
