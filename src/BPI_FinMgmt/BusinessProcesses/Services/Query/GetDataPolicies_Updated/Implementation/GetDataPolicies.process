                                                            </ns34:RecurringFrequency>
                                                        </ns34:Price>
                                                    </xsl:for-each>
                                                    <ns34:AssetStartDate>
                                                        <xsl:value-of select="if(tib:get-month-from-dateTime(AssetStartDate)=tib:get-month-from-dateTime(current-dateTime()) and tib:get-year-from-dateTime(AssetStartDate)=tib:get-year-from-dateTime(current-dateTime()))&#xA;then&#xA;AssetStartDate&#xA;else if(contains(AssetEndDate,'9999-12-31') and tib:get-month-from-dateTime(AssetStartDate)!=tib:get-month-from-dateTime(current-dateTime()))&#xA;then&#xA;concat(tib:left(current-date(),8),'01T00:00:00')&#xA;else&#xA;AssetStartDate"/>
                                                    </ns34:AssetStartDate>
                                                    <ns34:AssetEndDate>
                                                        <xsl:value-of select="if(contains(AssetEndDate,'9999-12-31'))&#xA;then&#xA;tib:format-dateTime(&quot;yyyy-MM-dd'T'HH:mm:ss&quot;,concat(tib:add-to-date(concat(tib:substring-before-last(current-date(), '-'),'-01'), 0, 1, -1),'T00:00:00-03:00'))&#xA;else&#xA;AssetEndDate"/>
                                                    </ns34:AssetEndDate>
                                                    <ns34:AssetActionCode>
                                                        <xsl:value-of select="$ProductId"/>
                                                    </ns34:AssetActionCode>
                                                    <xsl:if test="$CurrentId">
                                                        <ns34:AssetActionCommand>
                                                            <xsl:value-of select="$CurrentId"/>
                                                        </ns34:AssetActionCommand>
                                                    </xsl:if>
                                                    <xsl:if test="AssetStatus">
