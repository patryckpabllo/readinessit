    <pd:activity name="Map-Data-Offers-1">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>1324</pd:x>
        <pd:y>214</pd:y>
        <config>
            <element ref="ns33:DataInquiriesResponse"/>
        </config>
        <pd:inputBindings>
            <ns33:DataInquiriesResponse>
                <xsl:copy-of select="$Call_Get_UCUT/ns33:DataInquiriesResponse/ns11:BusinessMessageHeader"/>
                <xsl:copy-of select="$Call_Get_UCUT/ns33:DataInquiriesResponse/ns25:Response"/>
                <ns34:SubscriberAssets>
                    <xsl:copy-of select="$Call_Get_UCUT/ns33:DataInquiriesResponse/ns34:SubscriberAssets/ns34:Subscriber"/>
                    <ns34:ListOfAssets>
                        <xsl:for-each xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="$OfferToShow/ns31:GetOfferResponse/ns31:GetOfferRes/ns4:SubscriberAssets/ListOfAssets/Asset">
                            <xsl:if test="contains($ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=current()/AssetID]/ns35:OFFER_TYPE,'Data')">
                                <xsl:choose>
                                    <xsl:when test="$Call_Get_UCUT/ns33:DataInquiriesResponse/ns34:SubscriberAssets/ns34:ListOfAssets/ns34:Asset/ListOfServices/ns34:Service/ns34:ServiceType =current()/product/ProductID">
                                        <xsl:if test="$Call_Get_UCUT/ns33:DataInquiriesResponse/ns34:SubscriberAssets/ns34:ListOfAssets/ns34:Asset/ListOfServices/ns34:Service/ns34:ServiceType =current()/product/ProductID">
                                            <ns34:Asset>
                                                <xsl:variable name="CurrentId" select="current()/AssetID"/>
                                                <xsl:variable name="ProductId" select="current()/product/ProductID"/>
                                                <ns34:AssetID>
                                                    <xsl:value-of select="$ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[$CurrentId=ns35:OFFERID]/ns35:OFFER_NAME"/>
                                                </ns34:AssetID>
                                                <ns34:AssetType>
                                                    <xsl:value-of select="if(contains(AssetsDescription,'Voice'))&#xA;then&#xA;'Voice'&#xA;else&#xA;'Data'"/>
                                                </ns34:AssetType>
                                                <xsl:if test="$ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:CHARGING_TYPE">
                                                    <ns34:AssetsDescription>
                                                        <xsl:value-of select="$ESB_Offer_Mapping/ns35:ESBOfferMapping/ns35:Record/ns35:Records[ns35:OFFERID=$CurrentId]/ns35:CHARGING_TYPE"/>
                                                    </ns34:AssetsDescription>
                                                </xsl:if>
                                                <ns34:AssetAutoRenewalFlag>
                                                    <xsl:value-of select="AssetsDescription"/>
                                                </ns34:AssetAutoRenewalFlag>
                                                <ListOfServices>
                                                    <xsl:for-each select="$Call_Get_UCUT/ns33:DataInquiriesResponse/ns34:SubscriberAssets/ns34:ListOfAssets/ns34:Asset/ListOfServices/ns34:Service">
                                                        <xsl:if test="current()/ns34:ServiceID=$CurrentId and&#xA;$ProductId=current()/ns34:ServiceType">
                                                            <ns34:Service>
                                                                <ns34:ServiceID>
                                                                    <xsl:value-of select="ns34:ServiceID"/>
                                                                </ns34:ServiceID>
                                                                <xsl:if test="ns34:ServiceType">
                                                                    <ns34:ServiceType>
                                                                        <xsl:value-of select="ns34:ServiceType"/>
                                                                    </ns34:ServiceType>
