	public void invoke() throws Exception {
	String xmlOt="";
	String jsonOt="";
	if(getjsonIn()!=null)
	{
		JSONObject jObj = new JSONObject(getjsonIn());
		xmlOt=XML.toString(jObj);
                xmlOt="&lt;root&gt;" + xmlOt + "&lt;/root&gt;";
