    <pd:activity name="NativeToCDM">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>1101</pd:x>
        <pd:y>211</pd:y>
        <config>
            <element>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element ref="pfx32:FirstCallStatusChangeOutput"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:choose>
                    <xsl:when test="$CallEMA/outputMessage/pfx20:SetResponse/pfx20:MOAttributes/ns22:setResponseSubscription/ns22:respCode=0">
                        <pfx32:FirstCallStatusChangeOutput>
                            <pfx32:Status>
                                <xsl:value-of select="$CallEMA/outputMessage/pfx20:SetResponse/pfx20:MOAttributes/ns22:setResponseSubscription/ns22:respCode"/>
                            </pfx32:Status>
                            <pfx32:Description>
                                <xsl:value-of select="$CallEMA/outputMessage/pfx20:SetResponse/pfx20:MOAttributes/ns22:setResponseSubscription/ns22:respDesc"/>
                            </pfx32:Description>
                        </pfx32:FirstCallStatusChangeOutput>
                    </xsl:when>
                    <xsl:when test="not($CallEMA/outputMessage/pfx20:SetResponse/pfx20:MOAttributes/ns22:setResponseSubscription/ns22:respCode=0)">
                        <pfx32:FirstCallStatusChangeOutput>
                            <pfx32:Status>
                                <xsl:value-of select="$CallEMA/outputMessage/pfx20:SetResponse/pfx20:MOAttributes/ns22:setResponseSubscription/ns22:respCode"/>
                            </pfx32:Status>
                            <xsl:if test="$CallEMA/outputMessage/pfx20:SetResponse/pfx20:MOAttributes/ns22:setResponseSubscription/ns22:respDesc">
                                <pfx32:Description>
                                    <xsl:value-of select="$CallEMA/outputMessage/pfx20:SetResponse/pfx20:MOAttributes/ns22:setResponseSubscription/ns22:respDesc"/>
                                </pfx32:Description>
                            </xsl:if>
                        </pfx32:FirstCallStatusChangeOutput>
                    </xsl:when>
                    <xsl:otherwise>
                        <pfx32:FirstCallStatusChangeOutput>
                            <pfx15:ErrorResponse>
                                <pfx15:Error_SourceSystem>
                                    <xsl:value-of select="&quot;EMA&quot;"/>
                                </pfx15:Error_SourceSystem>
                                <xsl:if test="if(exists($_error_CallEMA/ActivityErrors/defaultFaultElement))&#xA;then $_error_CallEMA/ActivityErrors/defaultFaultElement/faultcode&#xA;else $_error_CallEMA/ActivityErrors/Cai3G12FaultException/faultcode">
                                    <pfx15:Error_SourceErrCode>
                                        <xsl:value-of select="if(exists($_error_CallEMA/ActivityErrors/defaultFaultElement))&#xA;then $_error_CallEMA/ActivityErrors/defaultFaultElement/faultcode&#xA;else $_error_CallEMA/ActivityErrors/Cai3G12FaultException/faultcode"/>
                                    </pfx15:Error_SourceErrCode>
                                </xsl:if>
                                <xsl:if test="if(exists($_error_CallEMA/ActivityErrors/defaultFaultElement))&#xA;then $_error_CallEMA/ActivityErrors/defaultFaultElement/faultstring&#xA;else $_error_CallEMA/ActivityErrors/Cai3G12FaultException/faultstring">
                                    <pfx15:Error_SourceErrDesc>
                                        <xsl:value-of select="if(exists($_error_CallEMA/ActivityErrors/defaultFaultElement))&#xA;then $_error_CallEMA/ActivityErrors/defaultFaultElement/faultstring&#xA;else $_error_CallEMA/ActivityErrors/Cai3G12FaultException/faultstring"/>
                                    </pfx15:Error_SourceErrDesc>
                                </xsl:if>
                                <pfx15:Error_Occurance_Time>
                                    <xsl:value-of select="current-dateTime()"/>
                                </pfx15:Error_Occurance_Time>
                                <xsl:if test="$ErrorcodeLookup/root/errorGroup/RETRYFLAG">
                                    <pfx15:Error_Additional_Desc1>
                                        <xsl:value-of select="$ErrorcodeLookup/root/errorGroup/RETRYFLAG"/>
                                    </pfx15:Error_Additional_Desc1>
                                </xsl:if>
                                <pfx15:Error_Additional_Desc2>
                                    <xsl:value-of select="tib:render-xml($_error/ns8:ErrorReport/Data)"/>
                                </pfx15:Error_Additional_Desc2>
                            </pfx15:ErrorResponse>
                        </pfx32:FirstCallStatusChangeOutput>
                    </xsl:otherwise>
                </xsl:choose>
            </root>
        </pd:inputBindings>
    </pd:activity>
