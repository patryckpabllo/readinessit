<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.zain.com/ZESB/Services/Inventory/QueryStoreRequest.xsd"
	 targetNamespace="http://www.zain.com/ZESB/Services/Inventory/QueryStoreRequest.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="QueryStoreRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="storeCode" type="xs:string">
					<xs:annotation>
						<xs:documentation>Warehouse Code in Zain ERP</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>